# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /ws                                         com.ddp.dataplatform.DataPlatformController.ws
GET         /sp/edit/:name                              com.ddp.dataplatform.DataPlatformController.get(name:String)
POST        /sp/edit                                    com.ddp.dataplatform.DataPlatformController.createOrUpdate

GET         /*file                                      com.ddp.dataplatform.Static.at(path="/public", file)
